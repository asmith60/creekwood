{
  "name": "creekwood",
  "version": "1.0.0",
  "description": "Creekwood game",
  "main": "dist/main.bundle.js",
  "sideEffects": false,
  "scripts": {
    "build": "webpack-cli",
    "dev": "webpack-dev-server",
    "test:lint": "npm run lint",
    "lint": "eslint --fix src test --ext .js,.jsx,.ts,.tsx",
    "lint:junit": "mkdir -p artifacts/junit/eslint && eslint --fix src test --ext .js,.jsx,.ts,.tsx -f junit > artifacts/junit/eslint/lint-test-results.xml",
    "start": "npm run dev"
  },
  "author": "“Adam <“asmith60@gmail.com”> (https://github.com/asmith60)",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^12.0.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.0.2",
    "eslint": "7.2.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "raw-loader": "4.0.1",
    "ts-loader": "7.0.5",
    "typescript": "3.9.5",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "phaser": "3.24.1"
  }
}